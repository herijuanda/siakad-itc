<script type="text/javascript">

    const path = '<%=path%>';

    $.ajaxSetup({
        headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
        }
    });

    $(document).on('click', '.detail', function () {
        
        var id  = $(this).data('id');

        $.post(url+'/detail', { id, path }, function (data) {
            $("#modal-label").html('Detail Nilai');
            $("#show-modal").html(data);
        });
    });

</script>