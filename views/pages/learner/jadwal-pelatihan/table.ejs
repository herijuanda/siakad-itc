<table class="table table-bordered">
    <thead>
        <tr>
            <th>Waktu</th>
            <th>Mata Pelatihan</th>
            <th>Kode Kelas</th>
            <th>Kelas</th>
            <th>Ruangan</th>
            <th>Instruktur</th>
        </tr>
    </thead>
    <tbody>
        <% if (data.length < 1) { %>
            <tr>
                <td colspan="6" class="text-center">Data Tidak Ditemukan</td>
            </tr>
        <% } %>

        <% data.forEach(function(value, i){ %>
            <% if(value?.m_day?.indonesian != data?.[(i-1)]?.m_day?.indonesian) { %> 
                <tr bgcolor="#f1f2f3">
                    <td colspan="6">
                        <b>
                            <%= value?.m_day?.indonesian%>
                        </b>
                    </td>
                </tr>
            <% } %> 
            <tr>
                <td><%= moment(value?.time_first, "HH:mm:ss").locale("id").format("LT") + ' s/d ' + moment(value?.time_last, "HH:mm:ss").locale("id").format("LT") %></td>
                <td><%= value?.d_classroom?.m_subject?.name || '-' %></td>
                <td><%= value?.d_classroom?.code || '-' %></td>
                <td><%= value?.d_classroom?.name || '-' %></td>
                <td><%= value?.room || '-' %></td>
                <td><%= value?.d_classroom?.m_lecturer?.user?.name || '-' %></td>
            </tr>
        <% }); %>
    </tbody>
</table>